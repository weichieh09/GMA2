<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity CountryStd.
    -->
    <changeSet id="20230402044446-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="country_id"
                                 baseTableName="country_std"
                                 constraintName="fk_country_std__country_id"
                                 referencedColumnNames="id"
                                 referencedTableName="country"/>

        <addForeignKeyConstraint baseColumnNames="std_id"
                                 baseTableName="country_std"
                                 constraintName="fk_country_std__std_id"
                                 referencedColumnNames="id"
                                 referencedTableName="std"/>
    </changeSet>
</databaseChangeLog>
