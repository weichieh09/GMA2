<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity ProdCountry.
    -->
    <changeSet id="20230402044442-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="prod_id"
                                 baseTableName="prod_country"
                                 constraintName="fk_prod_country__prod_id"
                                 referencedColumnNames="id"
                                 referencedTableName="prod"/>

        <addForeignKeyConstraint baseColumnNames="country_id"
                                 baseTableName="prod_country"
                                 constraintName="fk_prod_country__country_id"
                                 referencedColumnNames="id"
                                 referencedTableName="country"/>
    </changeSet>
</databaseChangeLog>
