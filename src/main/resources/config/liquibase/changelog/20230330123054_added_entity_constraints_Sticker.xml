<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Sticker.
    -->
    <changeSet id="20230330123054-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="sticker_id"
                                 baseTableName="rel_sticker__mark"
                                 constraintName="fk_rel_sticker__mark__sticker_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sticker"/>

        <addForeignKeyConstraint baseColumnNames="mark_id"
                                 baseTableName="rel_sticker__mark"
                                 constraintName="fk_rel_sticker__mark__mark_id"
                                 referencedColumnNames="id"
                                 referencedTableName="mark"/>

        <addForeignKeyConstraint baseColumnNames="prod_id"
                                 baseTableName="sticker"
                                 constraintName="fk_sticker__prod_id"
                                 referencedColumnNames="id"
                                 referencedTableName="prod"/>
    </changeSet>
</databaseChangeLog>
