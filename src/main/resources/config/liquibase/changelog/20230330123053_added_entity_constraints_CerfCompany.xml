<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity CerfCompany.
    -->
    <changeSet id="20230330123053-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="cerf_id"
                                 baseTableName="cerf_company"
                                 constraintName="fk_cerf_company__cerf_id"
