<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity CountryCert.
    -->
    <changeSet id="20230402044449-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="country_id"
                                 baseTableName="country_cert"
                                 constraintName="fk_country_cert__country_id"
                                 referencedColumnNames="id"
                                 referencedTableName="country"/>

        <addForeignKeyConstraint baseColumnNames="cerf_id"
                                 baseTableName="country_cert"
                                 constraintName="fk_country_cert__cerf_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cerf"/>
    </changeSet>
</databaseChangeLog>
