<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity ProdStd.
    -->
    <changeSet id="20230402044443-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="prod_id"
                                 baseTableName="prod_std"
                                 constraintName="fk_prod_std__prod_id"
                                 referencedColumnNames="id"
                                 referencedTableName="prod"/>

        <addForeignKeyConstraint baseColumnNames="std_id"
                                 baseTableName="prod_std"
                                 constraintName="fk_prod_std__std_id"
                                 referencedColumnNames="id"
                                 referencedTableName="std"/>
    </changeSet>
</databaseChangeLog>
