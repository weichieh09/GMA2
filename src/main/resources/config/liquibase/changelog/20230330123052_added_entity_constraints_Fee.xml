<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Fee.
    -->
    <changeSet id="20230330123052-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="prod_id"
                                 baseTableName="fee"
                                 constraintName="fk_fee__prod_id"
                                 referencedColumnNames="id"
                                 referencedTableName="prod"/>

        <addForeignKeyConstraint baseColumnNames="cerf_id"
                                 baseTableName="fee"
                                 constraintName="fk_fee__cerf_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cerf"/>

        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="fee"
                                 constraintName="fk_fee__company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"/>
    </changeSet>
</databaseChangeLog>
