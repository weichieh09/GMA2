<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity CountryMark.
    -->
    <changeSet id="20230402044451-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="country_id"
                                 baseTableName="country_mark"
                                 constraintName="fk_country_mark__country_id"
                                 referencedColumnNames="id"
                                 referencedTableName="country"/>

        <addForeignKeyConstraint baseColumnNames="mark_id"
                                 baseTableName="country_mark"
                                 constraintName="fk_country_mark__mark_id"
                                 referencedColumnNames="id"
                                 referencedTableName="mark"/>
    </changeSet>
</databaseChangeLog>
