<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Mark.
    -->
    <changeSet id="20230330093814-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="country_id"
                                 baseTableName="mark"
                                 constraintName="fk_mark__country_id"
                                 referencedColumnNames="id"
                                 referencedTableName="country"/>

        <addForeignKeyConstraint baseColumnNames="cerf_id"
                                 baseTableName="mark"
                                 constraintName="fk_mark__cerf_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cerf"/>

        <addForeignKeyConstraint baseColumnNames="sticker_id"
                                 baseTableName="mark"
                                 constraintName="fk_mark__sticker_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sticker"/>
    </changeSet>
</databaseChangeLog>
